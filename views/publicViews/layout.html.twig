<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Flexauto | Accueil</title>

    <!-- core CSS -->
    <link href="{{ app.request.basepath }}/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/font-awesome.min.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/animate.min.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/prettyPhoto.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/main.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/responsive.css" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="{{ app.request.basepath }}/js/html5shiv.js"></script>
    <script src="{{ app.request.basepath }}/js/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="{{ app.request.basepath }}/images/ico/favicon.ico">
</head><!--/head-->

<body class="homepage">

<header id="header">

    <div class="top-bar">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-xs-4">
                    <div class="top-infos">
                        <ul>
                            <li><p><i class="fa fa-phone-square"></i> +32 555 555 555</p></li>
                            <li><p><i class="fa fa-envelope"></i> info@flexauto.be</p></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-6 col-xs-8">
                    <div class="social">
                        <ul class="social-share">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#"><i class="fa fa-skype"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div><!--/.container-->
    </div><!--/.top-bar-->

    <nav class="navbar navbar-inverse" role="banner">

        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('index') }}">
                    Flex Auto
                </a>
                <p class="text-center">Vente voitures d'occasion</p>
            </div>
            <div class="collapse navbar-collapse navbar-right">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="{{ path('index') }}">Accueil</a></li>
                    <li><a href="{{ path('services') }}">Nos Services</a></li>
                    <li><a href="{{ path('a_propos') }}">A propos</a></li>
                    <li><a href="{{ path('contact') }}">Contact</a></li>
                </ul>
            </div>
        </div><!--/.container-->
    </nav><!--/nav-->

</header><!--/header-->

{% block content %}{% endblock %}

<section id="bottom">
    <div class="container wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="600ms">
        <div class="row">
            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Société</h3>
                    <ul>
                        <li><a href="#">A propos</a></li>
                        <li><a href="#">Notre équipe</a></li>
                        <li><a href="#">Nous contacter</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->

            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Nos Services</h3>
                    <ul>
                        <li><a href="#">Achat de voitures d'occasions</a></li>
                        <li><a href="#">Vente et reprise de voitures d'occasions</a></li>
                        <li><a href="#">Vente pièces détachées </a></li>
                        <li><a href="#">Réparation automobile</a></li>
                        <li><a href="#">Dépannage</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->

            <div class="col-md-3 col-sm-6">
                <div class="widget">
                    <h3>Nos Partenaires</h3>
                    <ul>
                        <li><a href="#">Spirale-WebAgency</a></li>
                        <li><a href="#">Peugeot</a></li>
                        <li><a href="#">Citroën</a></li>
                    </ul>
                </div>
            </div><!--/.col-md-3-->
        </div>
    </div>
</section><!--/#bottom-->

<footer id="footer" class="midnight-blue">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                &copy; 2017 - Spirale-Web. Tous droits réservés.
            </div>
            <div class="col-sm-6">
                <ul class="pull-right">
                    <li><a href="#">Accueil</a></li>
                    <li><a href="#">Qui sommes nous?</a></li>
                    <li><a href="#">Contact</a></li>
                    <li><a href="{{ path('dashboard') }}">Administrateur</a></li>
                </ul>
            </div>
        </div>
    </div>
</footer><!--/#footer-->

<script src="{{ app.request.basepath }}/js/jquery.js"></script>
<script src="{{ app.request.basepath }}/js/bootstrap.min.js"></script>
<script src="{{ app.request.basepath }}/js/main.js"></script>
<script src="{{ app.request.basepath }}/js/wow.min.js"></script>
</body>
</html>